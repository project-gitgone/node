{
  "name": "@project-gitgone/node",
  "version": "26.2.1",
  "description": "GitGone Node.js library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "test": "tsx bin/test.ts",
    "lint": "biome check src tests",
    "format": "biome format --write src tests"
  },
  "keywords": [
    "node",
    "library",
    "gitgone",
    "project",
    "management",
    "developer",
    "workflow"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-gitgone/node.git"
  },
  "author": "Th√©o Lagache <theo@lagache.dev>",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "@japa/assert": "^4.0.1",
    "@japa/runner": "^4.1.0",
    "tsx": "^4.19.2",
    "@types/node": "^22.10.2",
    "@biomejs/biome": "2.4.2"
  },
  "dependencies": {},
  "packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8"
}
